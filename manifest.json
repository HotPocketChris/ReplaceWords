 {
	"manifest_version": 2,
	"name": "Replace Words",
	"version": "1.00",
	"description": "Replaces words on website with another word",
	"content_scripts": [
		{
		"matches": ["<all_urls>" ],
		"js": ["content.js"]
		}
	],
	
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "searchTool"
	}



}
